{
    "name": "sbtsdev/laravelclickuperrorreports",
    "description": "Package for sending error reports from laravel to Clickup",
    "license": "MIT",
    "authors": [
        {
            "name": "sbtsdev",
            "email": "webdesign@sbts.edu",
            "homepage": "https://github.com/sbtsdev/"
        }
    ],
    "homepage": "https://github.com/sbtsdev/laravelclickuperrorreports",
    "keywords": ["Laravel", "LaravelClickupErrorReports"],
    "require": {
        "illuminate/support": "~10"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~8.5",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "sbtsdev\\LaravelClickupErrorReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sbtsdev\\LaravelClickupErrorReports\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sbtsdev\\LaravelClickupErrorReports\\LaravelClickupErrorReportsServiceProvider"
            ],
            "aliases": {
                "LaravelClickupErrorReports": "sbtsdev\\LaravelClickupErrorReports\\Facades\\LaravelClickupErrorReports"
            }
        }
    }
}
